[server]
host-name=#####MASKED_HOSTNAME#####
use-ipv4=no
use-ipv6=yes
check-response-ttl=no
use-iff-running=no


[publish]
publish-addresses=yes
publish-hinfo=yes
publish-workstation=no
publish-domain=yes


[reflector]
enable-reflector=no
reflect-ipv=no

[rlimits]
#rlimit-as=
rlimit-core=0
rlimit-data=4194304
rlimit-fsize=0
rlimit-nofile=30
rlimit-stack=4194304
rlimit-nproc=3



